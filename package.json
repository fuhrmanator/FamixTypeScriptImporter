{
  "name": "ts2famix",
  "version": "1.4.1",
  "description": "A TypeScript to JSON importer for Moose 10.",
  "main": "dist/ts2famix-cli.js",
  "scripts": {
    "dev": "ts-node src/ts2famix-cli.ts",
    "debug": "node --inspect-brk node_modules/.bin/ts-node",
    "build": "tsc",
    "test": "jest --colors --silent",
    "local": "sudo npm i -g && ts2famix",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "doc": "typedoc && touch docs/.gitkeep",
    "coverage": "jest --colors --silent --coverage --coverageDirectory=coverage",
    "uml": "./src/generate_uml.sh"
  },
  "bin": {
    "ts2famix": "dist/ts2famix-cli-wrapper.js"
  },
  "keywords": [],
  "author": "",
  "nodemonConfig": {
    "ignore": [
      "dist/*"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@types/yargs": "^17.0.32",
    "eslint": "^9.7.0",
    "jest": "^29.7.0",
    "tplant": "3.1.0",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.5"
  },
  "dependencies": {
    "@types/lodash": "^4.17.7",
    "grapheme-splitter": "^1.0.4",
    "lodash": "^4.17.21",
    "ts-morph": "^22.0.0",
    "tslog": "^4.9.2",
    "tsutils": "^3.21.0",
    "typescript": "^5.5.4",
    "yargs": "^17.7.2"
  }
}
